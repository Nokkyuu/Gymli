{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/assets/*", "/icons/*", "/manifest.json", "/favicon.png"]
  },
  "globalHeaders": {
    "index.html": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/**/*.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.wasm": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**/*.json": {
      "Cache-Control": "public, max-age=86400"
    },
    "/favicon.png": {
      "Cache-Control": "public, max-age=86400"
    },
    "/icons/*": {
      "Cache-Control": "public, max-age=86400"
    }
  }
}